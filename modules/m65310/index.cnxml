  </m:mfrac><m:mi>+</m:mi>
<m:mfrac>
    <m:mn>685</m:mn>
    <m:mn>755</m:mn>
  </m:mfrac>
<m:mo>)</m:mo><m:mi> - </m:mi>
<m:mfrac>
    <m:mn>280</m:mn>
    <m:mn>755</m:mn>
  </m:mfrac><m:mi>=</m:mi>
<m:mfrac>
    <m:mn>710</m:mn>
    <m:mn>755</m:mn>
  </m:mfrac>
</m:math>
  </para>
 </solution>
</exercise>


<exercise id="element-105"><problem id="id41525686">
